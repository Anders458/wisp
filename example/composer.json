{
  "autoload": {
    "psr-4": {
      "Example\\": "src/",
      "Wisp\\Example\\": "src/"
    }
  },
  "config": {
    "process-timeout": 0,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "anders/wisp": "dev-master"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../",
      "options": {
        "symlink": true
      }
    }
  ],
  "scripts": {
    "console": "@php index.php",
    "dev": "symfony local:server:start --port=10000",
    "test": "@php index.php test:run",
    "test:route": "@php index.php test:run"
  },
  "require-dev": {
    "pestphp/pest": "^4.1",
    "symfony/browser-kit": "^7.3",
    "symfony/dom-crawler": "^7.3",
    "fakerphp/faker": "^1.24",
    "symfony/css-selector": "^7.3"
  }
}
