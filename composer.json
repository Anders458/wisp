{
   "name": "anders/wisp",
   "description": "Ergonomic Symfony 8 bundle for building APIs",
   "type": "symfony-bundle",
   "keywords": [ "symfony", "bundle", "api" ],
   "license": "MIT",
   "autoload": {
      "psr-4": {
         "Wisp\\": "src/"
      }
   },
   "require": {
      "php": "^8.4",
      "psr/cache": "^3.0",
      "symfony/cache": "^8.0",
      "symfony/config": "^8.0",
      "symfony/console": "^8.0",
      "symfony/dependency-injection": "^8.0",
      "symfony/event-dispatcher": "^8.0",
      "symfony/framework-bundle": "^8.0",
      "symfony/http-foundation": "^8.0",
      "symfony/http-kernel": "^8.0",
      "symfony/rate-limiter": "^8.0",
      "symfony/security-bundle": "^8.0",
      "symfony/validator": "^8.0"
   },
   "require-dev": {
      "fakerphp/faker": "^1.24",
      "phpstan/phpstan": "^2.0",
      "symfony/browser-kit": "^8.0",
      "symfony/dom-crawler": "^8.0",
      "symfony/var-dumper": "^8.0"
   },
   "config": {
      "sort-packages": true
   }
}
